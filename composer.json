{
    "name": "laravolt/thunderclap",
    "description": "Laravel CRUD generator",
    "keywords": ["laravel", "laravolt", "crud", "generator"],
    "homepage": "https://github.com/laravolt/thunderclap",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0",
        "laravolt/semantic-form": "^4.0.1",
        "laravolt/suitable": "^4.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Thunderclap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Thunderclap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Thunderclap\\ServiceProvider"
            ]
        }
    }
}
