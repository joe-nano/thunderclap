{
    "name": "laravolt/thunderclap",
    "description": "Laravel CRUD generator",
    "keywords": [
        "laravel",
        "laravolt",
        "crud",
        "generator"
    ],
    "homepage": "https://github.com/laravolt/thunderclap",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.1",
        "laravolt/packer":"dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Thunderclap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Thunderclap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
