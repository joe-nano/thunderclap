{
    "name": "laravolt/thunderclap",
    "description": "Laravel CRUD generator",
    "keywords": [
        "laravel",
        "laravolt",
        "crud",
        "generator"
    ],
    "homepage": "https://github.com/laravolt/thunderclap",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|6.*",
        "laravolt/semantic-form": "^2.2",
        "laravolt/suitable": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Thunderclap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Thunderclap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Thunderclap\\ServiceProvider"
            ]
        }
    }
}
